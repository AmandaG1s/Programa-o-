#include <stdio.h>
// Leia um n√∫mero inteiro e imprima o seu antecessor e o seu sucessor.
int main()
{
int num,antecessor,sucessor;
printf("Escreva um numero inteiro:");
scanf("%d",&num);
antecessor = -1;
sucessor = +1;
printf("Numero antecessor eh:%d\n",antecessor);
printf("Numero antecessor eh:%d\n",antecessor);
return 0;
    
}